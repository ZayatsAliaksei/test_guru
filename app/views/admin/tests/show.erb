<h1><b>Test:</b></h1>
<p><%= @test.title %> <%= link_to "Delete", test_path(@test), method: :delete, data: { confirm: "Are you sure?" } %></p>
<h3>Questions:</h3>
<p><%= link_to 'Create new question', new_test_question_path(@test) %></p>
<% @test.questions.each do |q| %>
  <b><%= q.body %></b>
  <%= link_to 'Show', question_path(q.id) %>
  <%= link_to 'Edit', edit_question_path(q.id) %> |
  <%= link_to "Delete", question_path(q.id), method: :delete, data: { confirm: "Are you sure?" } %>
  <br>
<% end %>
<%= link_to 'Main page', root_path %>
